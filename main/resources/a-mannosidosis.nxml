                  <tr>
                    <th id="hd_h_a-mannosidosis.T.summary_of_molecular_ge_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Gene&#x000a0;<sup>1</sup></th>
                    <th id="hd_h_a-mannosidosis.T.summary_of_molecular_ge_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Test Method</th>
                    <th id="hd_h_a-mannosidosis.T.summary_of_molecular_ge_1_1_1_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Variants Detected&#x000a0;<sup>2</sup></th>
