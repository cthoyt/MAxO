                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_2 hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">15%&#x000a0;<sup>5</sup></td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">YL</td>
              </tr>
              <tr>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">160-kb heterozygous duplication at Xp21&#x000a0;<sup>6,&#x000a0;7</sup></td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_2 hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_2_1" valign="middle" align="left" rowspan="1" colspan="1">Rare&#x000a0;<sup>8</sup></td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_2 hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">Rare&#x000a0;<sup>8</sup></td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">XL</td>
              </tr>
              <tr>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">Heterozygous <italic toggle="yes">DMRT1</italic>&#x000a0;<sup>9</sup> deletion</td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_2 hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_2_1" valign="middle" align="left" rowspan="1" colspan="1">Rare&#x000a0;<sup>10</sup></td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_2 hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">Rare&#x000a0;<sup>10</sup></td>
                <td headers="hd_h_gonad-dys-46xy.T.molecular_genetics_of_n_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">AD</td>
              </tr>
