                <td headers="hd_h_spg8.T.washc5_kiaa0196_pathogenic_varian_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.2087G&#x0003e;C</td>
                <td headers="hd_h_spg8.T.washc5_kiaa0196_pathogenic_varian_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Gly696Ala</td>
