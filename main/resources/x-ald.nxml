                  <td headers="hd_h_x-ald.T.plasma_very_long_chain_fatty_aci_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">0.01+0.004</td>
                  <td headers="hd_h_x-ald.T.plasma_very_long_chain_fatty_aci_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">0.07+0.03</td>
                  <td headers="hd_h_x-ald.T.plasma_very_long_chain_fatty_aci_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">0.04+0.02</td>
                </tr>
