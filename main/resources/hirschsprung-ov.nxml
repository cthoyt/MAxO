                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_5" valign="middle" align="left" rowspan="1" colspan="1">Rare</td>
                  </tr>
                  <tr>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1"><related-object link-type="booklink" source-id="gene" document-id="men2" document-type="chapter">MEN 2A</related-object>/FMTC</td>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">MTC, pheo, hyperpara- <break/>thyroidism&#x000a0;<sup>3</sup></td>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">AD</td>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">10q11.21 / <italic toggle="yes">RET</italic></td>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_5" valign="middle" align="left" rowspan="1" colspan="1">&#x02264;1%</td>
                  </tr>
                  <tr>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
                      <related-object link-type="booklink" source-id="gene" document-id="men2" document-type="chapter">MEN 2B</related-object>
                    </td>
                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">MTC, pheo, mucosal and intestinal neuromas, skeletal abnormalities, corneal changes</td>
