                    <td headers="hd_h_hirschsprung-ov.T.monogenic_syndromic_fo_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
                      <related-object link-type="booklink" source-id="gene" document-id="slo" document-type="chapter">Smith-Lemli-Opitz syndrome</related-object>
