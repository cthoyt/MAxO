                <td headers="hd_h_husa.T.selected_cfh_pathogenic_variants_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.3590T&#x0003e;C</td>
                <td headers="hd_h_husa.T.selected_cfh_pathogenic_variants_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Val1197Ala</td>
              </tr>
              <tr>
                <td headers="hd_h_husa.T.selected_cfh_pathogenic_variants_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1"><italic toggle="yes">CFH</italic> and <italic toggle="yes">CFHR1</italic> hybrid allele</td>
                <td headers="hd_h_husa.T.selected_cfh_pathogenic_variants_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1"/>
              </tr>
            </tbody>
