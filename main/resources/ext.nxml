                  <th id="hd_h_ext.T.extl_gene_family_1_1_1_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Reference</th>
                  <th id="hd_h_ext.T.extl_gene_family_1_1_1_4" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Comment</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td headers="hd_h_ext.T.extl_gene_family_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
