                <tr>
                  <td headers="hd_h_dystonia-ov.T.complex_dystonias_inherite_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
                    <related-object link-type="booklink" source-id="gene" document-id="hmdpc" document-type="chapter">Hypermanganesemia with dystonia, polycythemia, and cirrhosis</related-object>
                  </td>
                  <td headers="hd_h_dystonia-ov.T.complex_dystonias_inherite_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">AR</td>
