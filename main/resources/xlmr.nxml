                  <td headers="hd_h_xlmr.T.summary_of_molecular_genetic_test_1_1_1_3 hd_h_xlmr.T.summary_of_molecular_genetic_test_1_1_2_1" valign="middle" align="left" rowspan="1" colspan="1">&#x0003c;5%&#x000a0;<sup>5</sup></td>
                  <td headers="hd_h_xlmr.T.summary_of_molecular_genetic_test_1_1_1_3 hd_h_xlmr.T.summary_of_molecular_genetic_test_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">Unknown</td>
                </tr>
                <tr>
                  <td headers="hd_h_xlmr.T.summary_of_molecular_genetic_test_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1"/>
