                  <th id="hd_h_hemo-a.T.symptoms_related_to_severity_of_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Severity</th>
                  <th id="hd_h_hemo-a.T.symptoms_related_to_severity_of_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Factor VIII Clotting Activity&#x000a0;<sup>1</sup></th>
                  <th id="hd_h_hemo-a.T.symptoms_related_to_severity_of_1_1_1_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Symptoms</th>
                  <th id="hd_h_hemo-a.T.symptoms_related_to_severity_of_1_1_1_4" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Usual Age at Diagnosis</th>
                </tr>
