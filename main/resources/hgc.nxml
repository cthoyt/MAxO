                  <td headers="hd_h_hgc.T.summary_of_molecular_genetic_testi_1_1_1_1" rowspan="2" valign="middle" align="left" scope="row" colspan="1">
                    <italic toggle="yes">CDH1</italic>
                  </td>
                  <td headers="hd_h_hgc.T.summary_of_molecular_genetic_testi_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>2</sup></td>
                  <td headers="hd_h_hgc.T.summary_of_molecular_genetic_testi_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">30%-50%&#x000a0;<sup>3</sup></td>
