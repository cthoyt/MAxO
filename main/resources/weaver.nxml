              <tr>
                <th id="hd_h_weaver.T.ezh2_pathogenic_variants_discus_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1">DNA Nucleotide Change</th>
                <th id="hd_h_weaver.T.ezh2_pathogenic_variants_discus_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Protein Amino Acid Change</th>
