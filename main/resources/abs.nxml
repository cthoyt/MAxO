                <tr>
                  <td headers="hd_h_abs.T.molecular_genetic_testing_used_in_1_1_1_1" rowspan="2" valign="middle" align="left" scope="row" colspan="1">
                    <italic toggle="yes">POR</italic>
                  </td>
                  <td headers="hd_h_abs.T.molecular_genetic_testing_used_in_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>3</sup></td>
                  <td headers="hd_h_abs.T.molecular_genetic_testing_used_in_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">92%&#x000a0;<sup>4</sup></td>
                </tr>
