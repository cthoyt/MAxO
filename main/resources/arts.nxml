                  <tr>
                    <td headers="hd_h_arts.T.summary_of_molecular_genetic_test_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
                      <italic toggle="yes">PRPS1</italic>
                    </td>
                    <td headers="hd_h_arts.T.summary_of_molecular_genetic_test_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>4</sup></td>
