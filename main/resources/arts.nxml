                <th id="hd_h_arts.T.selected_prps1_variants_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">DNA Nucleotide Change</th>
                <th id="hd_h_arts.T.selected_prps1_variants_1_1_1_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Predicted Protein Change <break/>(Alias&#x000a0;<sup>1</sup>)</th>
                <th id="hd_h_arts.T.selected_prps1_variants_1_1_1_4" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Reference Sequences</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td headers="hd_h_arts.T.selected_prps1_variants_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
                  <bold>Benign</bold>
                </td>
                <td headers="hd_h_arts.T.selected_prps1_variants_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">c.336T&#x0003e;C</td>
