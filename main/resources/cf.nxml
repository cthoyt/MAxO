                  <th headers="hd_h_cf.T.residual_risk_to_an_individual_with_1_1_1_2" id="hd_h_cf.T.residual_risk_to_an_individual_with_1_1_2_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">40%</th>
