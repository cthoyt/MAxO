                  <td headers="hd_h_i-p.T.selected_ikbkg_pathogenic_allelic_1_1_1_1" valign="top" align="left" scope="row" rowspan="1" colspan="1">c.1167dupC</td>
                  <td headers="hd_h_i-p.T.selected_ikbkg_pathogenic_allelic_1_1_1_2" valign="top" align="left" rowspan="1" colspan="1">p.Glu390ArgfsTer5</td>
