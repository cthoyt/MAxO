                <tr>
                  <td headers="hd_h_i-p.T.selected_ikbkg_pathogenic_allelic_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.399-?_1260+?del<break/>(common 11.7-kb deletion)</td>
                  <td headers="hd_h_i-p.T.selected_ikbkg_pathogenic_allelic_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1"/>
                  <td headers="hd_h_i-p.T.selected_ikbkg_pathogenic_allelic_1_1_1_3" rowspan="2" valign="middle" align="left" colspan="1">
