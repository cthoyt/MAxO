              <tr>
                <td headers="hd_h_leopard.T.map2k1_pathogenic_allelic_vari_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.199G&#x0003e;A</td>
                <td headers="hd_h_leopard.T.map2k1_pathogenic_allelic_vari_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Asp67Asn</td>
