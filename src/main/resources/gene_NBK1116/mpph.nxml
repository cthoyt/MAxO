                  <tr>
                    <td headers="hd_h_mpph.T.akt3_variants_discussed_in_this_g_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.548T&#x0003e;A</td>
                    <td headers="hd_h_mpph.T.akt3_variants_discussed_in_this_g_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Val183Asp</td>
