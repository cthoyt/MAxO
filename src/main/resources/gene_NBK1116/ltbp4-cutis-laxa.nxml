                <tr>
                  <td headers="hd_h_ltbp4-cutis-laxa.T.summary_of_molecular_1_1_1_1" rowspan="2" valign="middle" align="left" scope="row" colspan="1">
                    <italic toggle="yes">LTBP4</italic>
                  </td>
                  <td headers="hd_h_ltbp4-cutis-laxa.T.summary_of_molecular_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>3</sup><hr/></td>
                  <td headers="hd_h_ltbp4-cutis-laxa.T.summary_of_molecular_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">17/17&#x000a0;<sup>4</sup><hr/></td>
                </tr>
