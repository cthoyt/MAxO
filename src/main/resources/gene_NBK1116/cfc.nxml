                  </td>
                  <td headers="hd_h_cfc.T.summary_of_molecular_genetic_testi_1_1_1_2" rowspan="2" valign="middle" align="left" colspan="1">~25%</td>
                  <td headers="hd_h_cfc.T.summary_of_molecular_genetic_testi_1_1_1_3 hd_h_cfc.T.summary_of_molecular_genetic_testi_1_1_2_1" valign="middle" align="left" rowspan="1" colspan="1">~100%</td>
                  <td headers="hd_h_cfc.T.summary_of_molecular_genetic_testi_1_1_1_3 hd_h_cfc.T.summary_of_molecular_genetic_testi_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">Unknown&#x000a0;<sup>6</sup></td>
