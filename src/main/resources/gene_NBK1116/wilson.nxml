                  <td headers="hd_h_wilson.T.clinical_findings_in_individual_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">+</td>
                  <td headers="hd_h_wilson.T.clinical_findings_in_individual_1_1_1_5" valign="middle" align="left" rowspan="1" colspan="1">&#x02013;</td>
                  <td headers="hd_h_wilson.T.clinical_findings_in_individual_1_1_1_6" valign="middle" align="left" rowspan="1" colspan="1">&#x02013;</td>
