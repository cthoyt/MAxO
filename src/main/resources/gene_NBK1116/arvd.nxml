                    <td headers="hd_h_arvd.T.selected_pathogenic_tmem43_varian_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Ser358Leu</td>
                    <td headers="hd_h_arvd.T.selected_pathogenic_tmem43_varian_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">
                      <ext-link xmlns:xlink="http://www.w3.org/1999/xlink" ext-link-type="uri" xlink:href="http://www.ncbi.nlm.nih.gov/nuccore/NM_024334.2">NM_024334.2</ext-link>
                      <break/>
                      <ext-link xmlns:xlink="http://www.w3.org/1999/xlink" ext-link-type="uri" xlink:href="http://www.ncbi.nlm.nih.gov/protein/NP_077310.1">NP_077310.1</ext-link>
                    </td>
