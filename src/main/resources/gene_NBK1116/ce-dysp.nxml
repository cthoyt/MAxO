                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_2" valign="middle" colspan="1" align="left" scope="row" rowspan="1">Skin laxity&#x000a0;<sup>5</sup></td>
                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">10</td>
                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">9</td>
              </tr>
              <tr>
                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_2" valign="middle" colspan="1" align="left" scope="row" rowspan="1">Recurrent lung infections</td>
                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">8</td>
                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">4</td>
              </tr>
              <tr>
                <td headers="hd_h_ce-dysp.T.clinical_features_of_cranioect_1_1_1_2" valign="middle" colspan="1" align="left" scope="row" rowspan="1">Polydactyly</td>
