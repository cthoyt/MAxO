                    <td headers="hd_h_ce-dysp.T.wdr35_pathogenic_variants_disc_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.1592T&#x0003e;C</td>
                    <td headers="hd_h_ce-dysp.T.wdr35_pathogenic_variants_disc_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Leu531Pro&#x000a0;<sup>1</sup></td>
