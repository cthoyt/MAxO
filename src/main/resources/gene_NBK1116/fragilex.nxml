                  <th id="hd_h_fragilex.T.types_of_fmr1_repeat_expansio_1_1_1_2" rowspan="2" valign="middle" align="left" scope="col" colspan="1" headers="hd_h_fragilex.T.types_of_fmr1_repeat_expansio_1_1_1_2">Number of CGG Trinucleotide Repeats</th>
                  <th id="hd_h_fragilex.T.types_of_fmr1_repeat_expansio_1_1_1_3" rowspan="2" valign="middle" align="left" scope="col" colspan="1" headers="hd_h_fragilex.T.types_of_fmr1_repeat_expansio_1_1_1_3">Methylation Status of <italic toggle="yes">FMR1</italic></th>
