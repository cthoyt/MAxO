                    <th headers="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_1_4" id="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_2_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Carrier Females</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td headers="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_1_1" rowspan="2" valign="middle" align="left" scope="row" colspan="1">
                      <italic toggle="yes">NR0B1</italic>
                    </td>
                    <td headers="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>4</sup></td>
                    <td headers="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">Sequence variants&#x000a0;<sup>5</sup></td>
                    <td headers="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_1_4 hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_2_1" valign="middle" align="left" rowspan="1" colspan="1">Nearly 50%&#x000a0;<sup>5,&#x000a0;6</sup></td>
                    <td headers="hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_1_4 hd_h_ahc.T.summary_of_molecular_genetic_testi_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">&#x0003e;90%&#x000a0;<sup>7</sup></td>
                  </tr>
                  <tr>
