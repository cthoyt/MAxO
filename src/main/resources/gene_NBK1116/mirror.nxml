              <fn id="mirror.TF.2.1">
                <label>1. </label>
                <p>
                  <xref ref-type="bibr" rid="mirror.REF.m_neret.2014a.1999">M&#x000e9;neret et al [2014a]</xref>
                </p>
              </fn>
              <fn id="mirror.TF.2.2">
