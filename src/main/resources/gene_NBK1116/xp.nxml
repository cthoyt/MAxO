                </tr>
                <tr>
                  <th headers="hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_1_2" id="hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_2_1" valign="middle" colspan="1" align="left" scope="colgroup" rowspan="1">US</th>
