                  <td headers="hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_1_2 hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">3%</td>
                  <td headers="hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_1_2 hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_2_3" valign="middle" align="left" rowspan="1" colspan="1"/>
                  <td headers="hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_1_3 hd_h_xp.T.molecular_genetic_testing_used_in_x_1_1_2_4" valign="middle" align="left" rowspan="1" colspan="1">15/15 alleles&#x000a0;<sup>6</sup></td>
