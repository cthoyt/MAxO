                  <th headers="hd_h_pik3ca-overgrowth.T.findings_in_mcap_syn_1_1_1_1" id="hd_h_pik3ca-overgrowth.T.findings_in_mcap_syn_1_1_2_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Major</th>
                  <th headers="hd_h_pik3ca-overgrowth.T.findings_in_mcap_syn_1_1_1_1" id="hd_h_pik3ca-overgrowth.T.findings_in_mcap_syn_1_1_2_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Supportive</th>
