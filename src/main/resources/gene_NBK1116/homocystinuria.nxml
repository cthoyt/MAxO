                    <td headers="hd_h_homocystinuria.T.molecular_genetic_testi_1_1_1_1" rowspan="2" valign="middle" align="left" scope="row" colspan="1">
                      <italic toggle="yes">CBS</italic>
                    </td>
                    <td headers="hd_h_homocystinuria.T.molecular_genetic_testi_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>3</sup></td>
