                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_8" valign="top" align="left" rowspan="1" colspan="1">Encephalocele</td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_3" valign="top" align="left" rowspan="1" colspan="1">Meckel syndrome&#x000a0;<sup>28</sup></td>
                </tr>
                <tr>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1"><italic toggle="yes">TMEM216</italic>&#x000a0;<sup>29</sup></td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_1" valign="middle" align="left" rowspan="1" colspan="1">(+)</td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">(+)</td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_3" valign="middle" align="left" rowspan="1" colspan="1">++</td>
