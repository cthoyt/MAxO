                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_5" valign="middle" align="left" rowspan="1" colspan="1">(+)</td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_6" valign="middle" align="left" rowspan="1" colspan="1">+</td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_7" valign="middle" align="left" rowspan="1" colspan="1">+</td>
                  <td headers="hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_1_2 hd_h_joubert.T.genes_associated_with_js_by_ph_1_1_2_8" valign="middle" align="left" rowspan="1" colspan="1">Cardiac findings; encephalocele</td>
