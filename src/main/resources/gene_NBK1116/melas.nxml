                  <td headers="hd_h_melas.T.clinical_features_of_110_individ_1_1_1_2" valign="middle" colspan="1" align="left" scope="row" rowspan="1">Diabetes mellitus<hr/></td>
                  <td headers="hd_h_melas.T.clinical_features_of_110_individ_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">2<hr/></td>
                  <td headers="hd_h_melas.T.clinical_features_of_110_individ_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">27<hr/></td>
