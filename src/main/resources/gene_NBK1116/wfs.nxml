                  <th headers="hd_h_wfs.T.summary_of_molecular_genetic_testi_1_1_1_4" id="hd_h_wfs.T.summary_of_molecular_genetic_testi_1_1_2_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">WFS-like</th>
                  <th headers="hd_h_wfs.T.summary_of_molecular_genetic_testi_1_1_1_4" id="hd_h_wfs.T.summary_of_molecular_genetic_testi_1_1_2_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">DFNA6/14/38</th>
                </tr>
              </thead>
              <tbody>
