                    <italic toggle="yes">WFS1</italic>
                  </td>
                  <td headers="hd_h_wfs.T.summary_of_molecular_genetic_testi_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>4</sup></td>
