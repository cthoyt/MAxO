                  <td headers="hd_h_tuberous-sclerosis.T.molecular_genetic_t_1_1_1_4 hd_h_tuberous-sclerosis.T.molecular_genetic_t_1_1_2_2" valign="middle" align="left" rowspan="1" colspan="1">~2%&#x000a0;<sup>8</sup></td>
                </tr>
                <tr>
                  <td headers="hd_h_tuberous-sclerosis.T.molecular_genetic_t_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">Unknown</td>
                  <td headers="hd_h_tuberous-sclerosis.T.molecular_genetic_t_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">10%&#x000a0;<sup>10,&#x000a0;11</sup></td>
                  <td headers="hd_h_tuberous-sclerosis.T.molecular_genetic_t_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">NA</td>
