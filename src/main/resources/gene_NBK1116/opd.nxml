                    <th id="hd_b_opd.T.otopalatodigital_spectrum_disord_1_1_1_1_5" align="left" valign="middle" rowspan="1" colspan="1">Scoliosis</th>
                    <th id="hd_b_opd.T.otopalatodigital_spectrum_disord_1_1_1_1_6" align="left" valign="middle" rowspan="1" colspan="1">Digits</th>
                    <th id="hd_b_opd.T.otopalatodigital_spectrum_disord_1_1_1_1_7" align="left" valign="middle" rowspan="1" colspan="1">Other</th>
                  </tr>
