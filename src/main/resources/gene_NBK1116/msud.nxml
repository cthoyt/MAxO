                </tr>
                <tr>
                  <td headers="hd_h_msud.T.clinical_phenotypes_of_maple_syru_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
                    <bold>Intermediate</bold>
                    <hr/>
                  </td>
                  <td headers="hd_h_msud.T.clinical_phenotypes_of_maple_syru_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">Variable<hr/></td>
                  <td headers="hd_h_msud.T.clinical_phenotypes_of_maple_syru_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">
                    <list list-type="bullet">
