                  <td headers="hd_h_msud.T.clinical_phenotypes_of_maple_syru_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">
                    <list list-type="bullet">
                      <list-item>
                        <p>Maple syrup odor of cerumen</p>
                      </list-item>
                      <list-item>
                        <p>Poor feeding</p>
                      </list-item>
                      <list-item>
                        <p>Irritability, lethargy</p>
                      </list-item>
                      <list-item>
                        <p>Opisthotonus</p>
                      </list-item>
                      <list-item>
                        <p>Focal dystonia</p>
                      </list-item>
                      <list-item>
                        <p>"Fencing," "bicycling"</p>
                      </list-item>
                      <list-item>
                        <p>Obtundation, coma</p>
                      </list-item>
                      <list-item>
                        <p>Central respiratory failure</p>
                      </list-item>
                    </list>
                    <hr/>
                  </td>
                  <td headers="hd_h_msud.T.clinical_phenotypes_of_maple_syru_1_1_1_4" valign="middle" align="left" rowspan="1" colspan="1">
                    <list list-type="bullet">
