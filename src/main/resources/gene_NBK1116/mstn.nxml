                <td headers="hd_h_mstn.T.selected_mstn_variants_1_1_1_2" valign="middle" colspan="1" align="left" scope="row" rowspan="1">c.458A&#x0003e;G</td>
                <td headers="hd_h_mstn.T.selected_mstn_variants_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">p.Lys153Arg</td>
              </tr>
              <tr>
                <td headers="hd_h_mstn.T.selected_mstn_variants_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">
