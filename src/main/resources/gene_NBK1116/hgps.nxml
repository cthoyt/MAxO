                  <th id="hd_h_hgps.T.classic_hgps_and_atypical_hgps_ca_1_1_1_4" valign="middle" align="left" scope="col" rowspan="1" colspan="1"># of Affected Persons Identified</th>
                  <th id="hd_h_hgps.T.classic_hgps_and_atypical_hgps_ca_1_1_1_5" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Reference</th>
                </tr>
              </thead>
