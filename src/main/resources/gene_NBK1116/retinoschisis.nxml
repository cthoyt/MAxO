                  <td headers="hd_b_retinoschisis.T.summary_of_molecular_gen_1_1_1_3 hd_b_retinoschisis.T.summary_of_molecular_gen_1_1_2_2" align="left" valign="middle" rowspan="1" colspan="1"/>
