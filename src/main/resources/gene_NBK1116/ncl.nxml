                  <td headers="hd_h_ncl.T.diagnostic_algorithm_for_ncl_disea_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">If autosomal dominant:</td>
                  <td headers="hd_h_ncl.T.diagnostic_algorithm_for_ncl_disea_1_1_1_3" valign="middle" align="left" rowspan="1" colspan="1">
                    <italic toggle="yes">DNAJC5</italic>
