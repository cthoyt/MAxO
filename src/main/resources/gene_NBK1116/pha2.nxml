                    <th id="hd_h_pha2.T.selected_wnk1_pathogenic_variants_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Predicted Protein Change</th>
                    <th id="hd_h_pha2.T.selected_wnk1_pathogenic_variants_1_1_1_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Reference Sequences</th>
                  </tr>
                </thead>
