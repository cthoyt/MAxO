                <th headers="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_5" id="hd_h_porphyria-var.T.biochemical_characterist_1_1_2_8" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Asx</th>
