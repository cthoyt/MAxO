                <th id="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_5" valign="middle" colspan="2" align="left" scope="colgroup" rowspan="1">Plasma Porphyrins</th>
              </tr>
              <tr>
                <th headers="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_2" id="hd_h_porphyria-var.T.biochemical_characterist_1_1_2_1" valign="middle" colspan="1" align="left" scope="colgroup" rowspan="1">Active</th>
                <th headers="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_2" id="hd_h_porphyria-var.T.biochemical_characterist_1_1_2_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Asx</th>
