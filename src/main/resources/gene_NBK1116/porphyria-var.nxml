                <th headers="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_3" id="hd_h_porphyria-var.T.biochemical_characterist_1_1_2_4" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Asx</th>
                <th headers="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_4" id="hd_h_porphyria-var.T.biochemical_characterist_1_1_2_5" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Active</th>
                <th headers="hd_h_porphyria-var.T.biochemical_characterist_1_1_1_4" id="hd_h_porphyria-var.T.biochemical_characterist_1_1_2_6" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Asx</th>
