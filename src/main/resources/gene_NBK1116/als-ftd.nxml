                  <tr>
                    <th id="hd_h_als-ftd.T.summary_of_molecular_genetic_t_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Gene&#x000a0;<sup>1</sup></th>
                    <th id="hd_h_als-ftd.T.summary_of_molecular_genetic_t_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Test Method</th>
