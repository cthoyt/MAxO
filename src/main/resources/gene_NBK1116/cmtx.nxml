              <thead>
                <tr>
                  <th id="hd_h_cmtx.T.summary_of_molecular_genetic_test_1_1_1_1" rowspan="2" valign="middle" align="left" scope="col" colspan="1" headers="hd_h_cmtx.T.summary_of_molecular_genetic_test_1_1_1_1">Gene&#x000a0;<sup>1</sup></th>
