import urllib2

URLPREFIX = 'https://www.ncbi.nlm.nih.gov'
urls = open('../resource/urllist.txt')
for url in urls:
    url = url.rstrip('\n')
##the following line does not work because NCBI forbids visiting
    response = urllib2.urlopen(URLPREFIX + url)
    webcontent = response.read()
    name = url.split('/')[4]
    f = open('../resource/geneReviewsPages/' + name + '.html', 'w')
    f.write(webcontent)
    f.close
urls.close
