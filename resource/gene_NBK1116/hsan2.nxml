                    <td headers="hd_h_hsan2.T.summary_of_molecular_genetic_tes_1_1_1_4" rowspan="4" valign="middle" align="left" colspan="1">Unknown</td>
                  </tr>
                  <tr>
                    <td headers="hd_h_hsan2.T.summary_of_molecular_genetic_tes_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">HSAN2B</td>
                    <td headers="hd_h_hsan2.T.summary_of_molecular_genetic_tes_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1"><italic toggle="yes">RETREG1</italic>&#x000a0;<sup>5</sup></td>
                  </tr>
