                    <td headers="hd_h_hsan2.T.summary_of_molecular_genetic_tes_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">HSAN2C</td>
                    <td headers="hd_h_hsan2.T.summary_of_molecular_genetic_tes_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">
                      <italic toggle="yes">KIF1A</italic>
                    </td>
                  </tr>
                  <tr>
                    <td headers="hd_h_hsan2.T.summary_of_molecular_genetic_tes_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">HSAN2D</td>
