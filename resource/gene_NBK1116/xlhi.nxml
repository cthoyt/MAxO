                    <th id="hd_h_xlhi.T.molecular_genetic_testing_used_in_1_1_1_3" rowspan="2" valign="middle" align="left" scope="col" colspan="1" headers="hd_h_xlhi.T.molecular_genetic_testing_used_in_1_1_1_3">Variants Detected&#x000a0;<sup>2</sup></th>
                    <th id="hd_h_xlhi.T.molecular_genetic_testing_used_in_1_1_1_4" valign="middle" colspan="2" align="left" scope="colgroup" rowspan="1">Variant Detection Frequency by Test Method&#x000a0;<sup>3</sup></th>
                  </tr>
