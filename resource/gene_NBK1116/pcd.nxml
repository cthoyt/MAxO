                    <td headers="hd_h_pcd.T.dnah11_variants_discussed_in_this_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.8554C&#x0003e;T</td>
                    <td headers="hd_h_pcd.T.dnah11_variants_discussed_in_this_1_1_1_2" valign="middle" align="left" rowspan="1" colspan="1">p.Arg2852Ter</td>
                  </tr>
