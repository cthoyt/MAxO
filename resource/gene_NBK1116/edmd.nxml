                  <tr>
                    <th id="hd_h_edmd.T.selected_lmna_pathogenic_variants_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1">DNA Nucleotide Change</th>
                    <th id="hd_h_edmd.T.selected_lmna_pathogenic_variants_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Predicted Protein Change</th>
