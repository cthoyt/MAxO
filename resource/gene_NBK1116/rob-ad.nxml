                  <tr>
                    <th id="hd_h_rob-ad.T.wnt5a_selected_pathogenic_varia_1_1_1_1" valign="middle" align="left" scope="col" rowspan="1" colspan="1">DNA Nucleotide Change<break/>(Alias&#x000a0;<sup>1</sup>)</th>
                    <th id="hd_h_rob-ad.T.wnt5a_selected_pathogenic_varia_1_1_1_2" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Protein Amino Acid Change<break/>(Alias&#x000a0;<sup>1</sup>)</th>
                    <th id="hd_h_rob-ad.T.wnt5a_selected_pathogenic_varia_1_1_1_3" valign="middle" align="left" scope="col" rowspan="1" colspan="1">Reference Sequences</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td headers="hd_h_rob-ad.T.wnt5a_selected_pathogenic_varia_1_1_1_1" valign="middle" align="left" scope="row" rowspan="1" colspan="1">c.257A&#x0003e;G&#x000a0;<sup>2</sup></td>
