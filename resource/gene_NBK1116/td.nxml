                  <th headers="hd_b_td.T.molecular_genetic_testing_used_in_t_1_1_1_4" id="hd_b_td.T.molecular_genetic_testing_used_in_t_1_1_2_2" align="left" valign="middle" rowspan="1" colspan="1">TD Type II</th>
