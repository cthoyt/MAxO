                </tr>
                <tr>
                  <td headers="hd_b_msx2.T.summary_of_molecular_genetic_test_1_1_1_1" align="left" valign="middle" rowspan="2" colspan="1"><italic toggle="yes">MSX2</italic>
</td>
                  <td headers="hd_b_msx2.T.summary_of_molecular_genetic_test_1_1_1_2" align="left" valign="middle" rowspan="2" colspan="1">~40%</td>
                  <td headers="hd_b_msx2.T.summary_of_molecular_genetic_test_1_1_1_3" align="left" valign="middle" rowspan="1" colspan="1">Sequence analysis&#x000a0;<sup>5</sup></td>
